   ],
   "source": [
    "# Pyber Challenge\n",
    "\n",
    "# Code Templates:\n",
    "# https://www.geeksforgeeks.org/creating-a-dataframe-from-pandas-series/\n",
    "# https://pandas.pydata.org/pandas-docs/stable/user_guide/style.html\n",
    "\n",
    "# pyber_data_df\n",
    "\n",
    "# Sum drivers by city type\n",
    "sum_drivers_by_type = city_data_df.groupby([\"type\"]).sum()[\"driver_count\"]\n",
    "\n",
    "# Sum Rides by city type\n",
    "sum_rides_by_type = pyber_data_df.groupby([\"type\"]).count()[\"ride_id\"]\n",
    "\n",
    "# Calculate avearge fare per ride\n",
    "avg_fare_per_ride = pyber_data_df.groupby('type').mean()['fare']\n",
    "\n",
    "# Calculate average fare per driver\n",
    "avg_fare_per_driver = sum_fares_by_type / sum_drivers_by_type\n",
    "\n",
    "# Creating summary data frame: total rides, total drivers, total fares, average fare per ride, average fare per driver\n",
    "summary = {'Total Rides': sum_rides_by_type, 'Total Drivers': sum_drivers_by_type,\n",
    "           'Total Fares': sum_fares_by_type, 'Average Fare Per Ride': avg_fare_per_ride,\n",
    "           'Average Fare Per Driver': (avg_fare_per_driver)}\n",
    "summary_df = pd.DataFrame(summary)\n",
    "\n",
    "# Formatting\n",
    "summary_df.style.format({'Total Rides': '{:,.0f}', 'Total Drivers': '{:,.0f}', 'Total Fares': '${:,.2f}',\n",
    "                        'Average Fare Per Ride': '${:,.2f}', 'Average Fare Per Driver': '${:,.2f}'})"
   ]
  },
  {
